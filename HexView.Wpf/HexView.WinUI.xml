        <member name="T:HexView.WinUI.Converters.EqualityConverter">
            <summary>
            A converter which tests for equality against a parameter.
            </summary>
        </member>
        <member name="M:HexView.WinUI.Converters.EqualityConverter.Convert(System.Object,System.Type,System.Object,System.Globalization.CultureInfo)">
             <summary>
             Tests the value against the parameter for equality.
             </summary>
            
             <param name="value">
             The value produced by the binding source.
             </param>
            
             <param name="targetType">
             The type of the binding target property which must be <see cref="T:System.Boolean"/>.
             </param>
            
             <param name="parameter">
             The converter parameter to use.
             </param>
            
             <param name="culture">
             The culture to use in the converter which is unused.
             </param>
            
             <returns>
             <c>true</c> if <paramref name="value"/> is equal to <paramref name="parameter"/>; <c>false</c> otherwise.
             </returns>
        </member>
        <member name="M:HexView.WinUI.Converters.EqualityConverter.ConvertBack(System.Object,System.Type,System.Object,System.Globalization.CultureInfo)">
